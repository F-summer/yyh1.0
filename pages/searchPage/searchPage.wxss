/* pages/searchPage/searchPage.wxss */
page{
  background-color: #f4f4f4;
  padding-top: 110rpx;
}
.indexAther{
  padding: 25rpx 32rpx;
  background-color: #fff;
  position: fixed;
  top: 0;
  left: 0;
}
.sea-add-cont{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-between;
}
.sea-add-cont .search{
  display: flex;
  align-items: center;
  width: 690rpx;
	height: 72rpx;
	background-color: #f4f4f4;
	border-radius: 36rpx;
  overflow: hidden;
  padding:0rpx 24rpx;
}
.sea-add-cont .search .search-icon{
  width: 27rpx;
  height: 28rpx;
  margin-right: 16rpx;
}
.sea-add-cont .search .search-inp{
  font-size: 30rpx;
	color: #333;
   width: 610rpx;
}
.sea-add-cont .cancer-btn{
	border-radius: 36rpx;
  font-size: 30rpx;
	line-height: 72rpx;
	color: #333333;
  white-space: nowrap;
}